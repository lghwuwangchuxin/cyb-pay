<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.parking.dao.SequenceDao" >
	<select id="getSeqIncrement" resultType="java.lang.String" parameterType="java.lang.String"> 
    	SELECT increment FROM sequence WHERE name=#{name}
	</select>
	
	<select id="getSequenceTradeId" resultType="java.lang.String">
    	SELECT NEXTVAL('TRADE_ID');
	</select>

	<select id="getSequenceOrderId" resultType="java.lang.String"> 
	     select seq('ORDERID');
	</select>
</mapper> 