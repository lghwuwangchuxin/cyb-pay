  <insert id="insertParkingChannelQrcodeTerminPayRouteConfig" parameterType="com.parking.domain.ParkingChannelQrcodeTerminPayRouteConfig">
      insert into t_parking_qrcode_termin_pay_route_config (PARK_ID,TERM_ID,STATES,
      RSRV_STR1,RSRV_STR2,RSRV_STR3,RSRV_STR4,RSRV_STR5,RSRV_STR6,REMARK,CREATED_TIME,UPDATE_TIME,
