<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.parking.dao.ImageDao">

    <insert id="addImage" parameterType="com.parking.domain.Image" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO image(`description`,`fileName`) VALUES(#{description},#{fileName})
    </insert>

    <select id="getAllImages" resultType="com.parking.domain.Image">
        SELECT * FROM image
    </select>
</mapper>